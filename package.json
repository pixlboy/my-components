{
	"name": "NGCL",
	"title": "Next Generation Components Library using ReactJS",
	"version": "1.0.0",
	"author": {
		"name": "Frank He",
		"email": "frankhe@juniper.net"
	},
	"scripts": {
		"start": "concurrently \"npm run tsc:w\" \"npm run build:js\" \"npm run min:js\"  \"npm run lite\"",
		"lite": "lite-server -c bs-config.json",
		"tsc": "tsc",
		"tsc:w": "tsc -w",
		"build:js": "webpack -d --watch",
		"min:js": "webpack --config webpack.config.min.js --watch",
		"typings": "typings",
		"postinstall": "typings install",
		"babel": "babel --presets es2015,react --watch public/src/ --out-dir public/build/",
		"test": "jest --coverage",
		"test-faked": "jest --testPathPattern=timezoneService_test.js"
	},
	"license": "ISC",
	"dependencies": {
		"angular2": "2.0.0-beta.14",
		"babel-runtime": "^6.9.0",
		"bootstrap": "^3.3.6",
		"d3": "^3.5.3",
		"dialog-polyfill": "^0.4.7",
		"es6-shim": "^0.35.0",
		"evaporate": "^2.1.0",
		"extract-text-webpack-plugin": "^1.0.1",
		"fixed-data-table-2": "0.8.5",
		"font-awesome": "^4.3.0",
		"immutable": "^3.8.1",
		"material-design-lite": "^1.3.0",
		"perfect-scrollbar": "^0.6.11",
		"prop-types": "15.5.8",
		"react": "15.3.2",
		"react-dom": "15.3.2",
		"react-mdl": "1.9.0",
		"reflect-metadata": "0.1.2",
		"rxjs": "5.0.0-beta.2",
		"systemjs": "0.19.25",
		"underscore": "^1.8.3",
		"xlsx": "^0.12.1",
		"zone.js": "0.6.6"
	},
	"devDependencies": {
		"autoprefixer": "^6.3.6",
		"babel": "^6.5.2",
		"babel-core": "^6.9.0",
		"babel-jest": "^9.0.3",
		"babel-loader": "^6.2.4",
		"babel-plugin-syntax-trailing-function-commas": "^6.22.0",
		"babel-plugin-transform-object-assign": "^6.22.0",
		"babel-plugin-transform-object-rest-spread": "^6.23.0",
		"babel-plugin-transform-react-jsx": "^6.8.0",
		"babel-plugin-transform-runtime": "^6.9.0",
		"babel-polyfill": "^6.22.0",
		"babel-preset-es2015": "^6.22.0",
		"babel-preset-react": "^6.22.0",
		"babelify": "^7.2.0",
		"classnames": "^2.2.5",
