		"extract-text-webpack-plugin": "^1.0.1",
		"faker": "^2.1.2",
		"file-loader": "^0.8.1",
		"flux": "^2.0.1",
		"imports-loader": "^0.6.5",
		"jest": "18.1.0",
		"jquery": "^2.2.3",
		"jquery-ui": "github:jquery/jquery-ui#1.11.4",
		"json-loader": "^0.5.4",
		"keymirror": "~0.1.0",
		"lite-server": "^2.2.0",
		"node-sass": "^3.13.1",
		"object-assign": "^1.0.0",
